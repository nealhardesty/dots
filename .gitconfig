[user]
	email = nealhardesty@yahoo.com
	name = Neal Hardesty
[github]
	user = nealhardesty
[core]
	editor = vim
[diff]
	tool = vimdiff
[alias]
  ci = commit
  co = checkout
  w = whatchanged
  s = status
  a = add
	remotes = remote -v
	branches = branch -a
  crap = reset HEAD --hard
  history = log --oneline --decorate --first-parent -n 100
  graph = log --graph --oneline --decorate
[push]
	default = matching
